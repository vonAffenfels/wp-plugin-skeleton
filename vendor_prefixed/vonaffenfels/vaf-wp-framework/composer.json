{
    "name": "vonaffenfels\/vaf-wp-framework",
    "type": "project",
    "description": "Framework to simplify plugin development",
    "homepage": "https:\/\/github.com\/vonAffenfels\/vaf-wp-framework",
    "authors": [
        {
            "name": "Christoph Friedrich",
            "email": "christoph.friedrich@vonaffenfels.de",
            "homepage": "https:\/\/www.vonaffenfels.de"
        }
    ],
    "support": {
        "issues": "https:\/\/github.com\/vonAffenfels\/vaf-wp-framework\/issues"
    },
    "keywords": [
        "composer",
        "wordpress",
        "wp",
        "vonaffenfels",
        "vaf",
        "framework"
    ],
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "platform": {
            "php": "8.1"
        },
        "sort-packages": true,
        "allow-plugins": {
            "pestphp\/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "WPPluginSkeleton_Vendor\\VAF\\WP\\Framework\\": "src\/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WPPluginSkeleton_Vendor\\VAF\\WP\\FrameworkTests\\": "tests\/"
        }
    },
    "scripts": {
        "test": [
            "pest"
        ],
        "codestyle": [
            "phpcs --standard=PSR12 src ; phpcs --standard=tests\/.codestyle.xml --extensions=php tests"
        ],
        "fixstyle": [
            "phpcbf --standard=PSR12 --extensions=php src ; phpcbf --standard=tests\/.codestyle.xml --extensions=php tests"
        ]
    },
    "require": {
        "php": ">=8.1",
        "symfony\/config": "^6.2",
        "symfony\/dependency-injection": "^6.2",
        "symfony\/yaml": "^6.2",
        "twig\/twig": "^3"
    },
    "require-dev": {
        "composer\/composer": "^2.6",
        "friendsofphp\/php-cs-fixer": "^3.22",
        "humbug\/php-scoper": "^0.18.3",
        "mockery\/mockery": "^1.6",
        "pestphp\/pest": "^2.36",
        "pestphp\/pest-plugin-drift": "^2.5",
        "squizlabs\/php_codesniffer": "^3.6"
    }
}